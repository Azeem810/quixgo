    "@emotion/styled": "^11.11.0",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@next/font": "^13.5.6",
    "@react-google-maps/api": "^2.19.2",
    "axios": "^1.6.3",
    "env-cmd": "^10.1.0",
    "framer-motion": "^8.5.5",
    "google-map-react": "^2.2.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-slick": "^0.29.0",
    "react-toastify": "^9.1.3",
    "sharp": "^0.31.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "sass": "^1.69.6",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1"
  }
